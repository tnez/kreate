{
  "name": "@tnez/kreate",
  "version": "0.0.0",
  "keywords": [
    "starter",
    "template",
    "cli",
    "nodejs"
  ],
  "description": "A CLI to bootstrap @tnez projects from templates.",
  "repository": {
    "type": "git",
    "url": "https://github.com/tnez/kreate"
  },
  "author": "Travis Nesland <travis@tnez.dev>",
  "license": "MIT",
  "scripts": {
    "dev": "ncc build ./index.ts -w -o dist/",
    "prerelease": "rimraf ./dist/",
    "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "republish": "npm release"
  },
  "devDependencies": {
    "@types/degit": "2.8.3",
    "@types/node": "17.0.32",
    "@vercel/ncc": "0.33.4",
    "commander": "9.2.0",
    "degit": "2.8.4",
    "rimraf": "3.0.2",
    "typescript": "4.6.4"
  },
  "engines": {
    "node": ">=12.22.0"
  }
}
